{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "angryadf"
		},
		"sqlAdventureWorksSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqlAdventureWorksSource'"
		},
		"sqldwAdventureWorksDestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldwAdventureWorksDestination'"
		},
		"blobstagingaccount_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobstagingaccount'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sqlAdventureWorksSource_connectionString": {
						"value": "[parameters('sqlAdventureWorksSource_connectionString')]"
					},
					"sqldwAdventureWorksDestination_connectionString": {
						"value": "[parameters('sqldwAdventureWorksDestination_connectionString')]"
					},
					"blobstagingaccount_connectionString": {
						"value": "[parameters('blobstagingaccount_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}